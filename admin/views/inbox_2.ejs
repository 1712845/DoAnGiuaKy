<!-- script-for sticky-nav -->
		<script>
		$(document).ready(function() {
			 var navoffeset=$(".header-main").offset().top;
			 $(window).scroll(function(){
				var scrollpos=$(window).scrollTop(); 
				if(scrollpos >=navoffeset){
					$(".header-main").addClass("fixed");
				}else{
					$(".header-main").removeClass("fixed");
				}
			 });
			 
		});
		</script>
		<!-- /script-for sticky-nav -->
<!--inner block start here-->
<div class="inner-block">
    <div class="inbox">
    	  <h2>Soạn email</h2>
    	 <div class="col-md-4 compose">   	 	
    	 	<div class="mail-profile">
    	 		<div class="mail-pic">
    	 			<a href="#"><img src="images/b3.png" alt=""></a>
    	 		</div>
    	 		<div class="mailer-name"> 			
    	 				<h5><a href="#">Nguyễn Ngọc Trung</a></h5>  	 				
    	 			     <h6><a href="mailto:info@example.com">nnt@gmail.com</a></h6>   
    	 		</div>
    	 	    <div class="clearfix"> </div>
    	 	</div>
    	 	<div class="compose-bottom">
    	 		<ul>
    	 			<li><a class="hilate" href="#"><i class="fa fa-inbox"> </i>Tin nhắn</a></li>
    	 			<li><a href="#"><i class="fa fa-envelope-o"> </i>Gửi mail</a></li>
    	 			<li><a href="#"><i class="fa fa-star-o"> </i>Quan trọng</a></li>
    	 			<li><a href="#"><i class="fa fa-pencil-square-o"> </i>Bản nháp</a></li>
    	 			<li><a href="#"><i class="fa fa-trash-o"> </i>Thùng rác</a></li>
    	 		</ul>
    	 	</div>
    	 </div>   	 
    	 	<div class="col-md-8 compose-right">
					<div class="inbox-details-default">
						<div class="inbox-details-heading">
							Soạn tin nhắn mới
						</div>
						<div class="inbox-details-body">
							<div class="alert alert-info">
                                Vui lòng điền chi tiết để gửi tin nhắn mới
							</div>
							<form class="com-mail">
								<input type="text"  value="Đến" :" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'To';}">
								<input type="text"  value="Tiêu đề :" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Subject';}">
								
								<textarea rows="6"  value="Tin nhắn :" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Message';}">Message </textarea>
								<div class="form-group">
									<div class="btn btn-default btn-file">
										<i class="fa fa-paperclip"> </i> Tập tin đính kèm
										<input type="file" name="attachment">
									</div>
								</div>
								<input type="submit" value="Gửi tin nhắn"> 
							</form>
						</div>
					</div>
				</div>
    	
          <div class="clearfix"> </div>     
   </div>
</div>

